<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cse.smartmotion"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cse.smartmotion.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cse.smartmotion.CreateAccount"
            android:label="Create Account" >
        </activity>
        <activity
            android:name="com.cse.smartmotion.Main_menu"
            android:label="@string/title_activity_main_menu" >
        </activity>
        <activity
            android:name="com.cse.smartmotion.New_Gesture"
            android:label="@string/title_activity_new__gesture" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cse.smartmotion.Main_menu" />
        </activity>
        <activity
            android:name="com.cse.smartmotion.Settings"
            android:label="@string/title_activity_settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cse.smartmotion.Main_menu" />
        </activity>
    </application>

</manifest>
